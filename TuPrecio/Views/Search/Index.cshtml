@model List<TuPrecio.Models.Article>
@{
    ViewBag.Title = "Index";
}


<div class="row">
    @if (Model == null || !Model.Any())
    {
        <h2>No results!</h2>
    }
    else
    {
        <table>
            <thead>
                <tr>
                    <th>Nombre</th>
                    <th width="150">Precio</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var a in Model)
                {
                    <tr>
                        <td>@a.Name</td>
                        <td>@a.Price</td>
                    </tr>
                }
            </tbody>
        </table>
    }
</div>
